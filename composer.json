{
  "name": "jop-software/doctrine-uuid-generators",
  "description": "Generate UUIDs for your doctrine models",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "jop-software Inh. Johannes Przymusinski",
      "email": "info@jop-software.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^3.0",
    "symfony/uid": "^5.3 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "JopSoftware\\DoctrineUuidGenerators\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JopSoftware\\DoctrineUuidGenerators\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 | ^10.0",
    "symplify/easy-coding-standard": "^11.1",
    "phpstan/phpstan": "^1.8"
  }
}
